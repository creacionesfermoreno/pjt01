<!DOCTYPE html>
<html lang="zxx">
<head>
    <meta charset="UTF-8">
    <title>Appsfit</title>
    @if (IsSectionDefined("Styles"))
    {@RenderSection("Styles", required: false)}
    <!-- =================== META =================== -->
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="format-detection" content="telephone=no">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="shortcut icon" href="assets/img/favicon.png">
    <!-- =================== STYLE =================== -->
    <link href="@Url.Content("~/Content/fitmax/css/slick.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/fitmax/css/bootstrap-grid.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/fitmax/css/font-awesome.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/fitmax/css/jquery.fancybox.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/fitmax/css/style.css")" rel="stylesheet" />

</head>

<body id="home">
    <!--================ PRELOADER ================-->
    <div class="preloader-cover">
        <div id="cube-loader">
            <div class="caption">
                <div class="cube-loader">
                    <div class="cube loader-1"></div>
                    <div class="cube loader-2"></div>
                    <div class="cube loader-4"></div>
                    <div class="cube loader-3"></div>
                </div>
            </div>
        </div>
    </div>
    <!--============== PRELOADER END ==============-->
    <!-- ================= HEADER ================= -->
    <header class="header">
        <a href="#" class="nav-btn">
            <span></span>
            <span></span>
            <span></span>
        </a>

        <div class="header-menu">
            <div class="container">
                <div class="header-logo">
                    <a href="" class="logo"><img src="~/Content/fitmax/img/logo-2.svg" alt="logo"></a>
                </div>
                <nav class="nav-menu">
                    <ul class="nav-list">
                        <li>
                            <a href="@Url.Action("inicio","pg", new {id= "INNOVAPERU"})">INICIO</a>
                        </li>
                        <li><a href="@Url.Action("reservaonline","pg", new {id= "INNOVAPERU"})">RESERVAS DE CLASES</a></li>
                        <li><a href="@Url.Action("tienda","pg", new {id= "INNOVAPERU"})">TIENDA</a></li>
                        <li><a href="@Url.Action("entrenaonline","pg", new {id= "INNOVAPERU"})">ENTRENA ONLINE</a></li>
                        <li><a href="@Url.Action("videoteca","pg", new {id= "INNOVAPERU"})">VIDEOTECA</a></li>
                        <li><a href="@Url.Action("login","pg", new {id= "INNOVAPERU"})">LOGIN</a></li>
                    </ul>
                </nav>
            </div>
        </div>
    </header>


    @RenderBody()
    <footer class="footer-fitness">
        <div class="container">           
            <div class="footer-bottom">
                <div class="copyright"><a href="#" target="_blank">Appfits</a> © 2020. Appfits. All Rights Reserved.</div>
            </div>
        </div>
    </footer>
    <!-- ================ FOOTER END ================ -->
    <!--=================== TO TOP ===================-->
    <a class="to-top" href="#home">
        <i class="fa fa-chevron-up" aria-hidden="true"></i>
    </a>
    <!--================= TO TOP END =================-->
    <!--=================== SCRIPT	===================-->
    @System.Web.Optimization.Scripts.Render("~/Content/fitmax/js/jquery-2.2.4.min.js")
    @System.Web.Optimization.Scripts.Render("~/Content/fitmax/js/slick.min.js")
    @System.Web.Optimization.Scripts.Render("~/Content/fitmax/js/isotope.pkgd.js")
    @System.Web.Optimization.Scripts.Render("~/Content/fitmax/js/jquery.fancybox.js")
    @System.Web.Optimization.Scripts.Render("~/Content/fitmax/js/rx-lazy.js")
    @System.Web.Optimization.Scripts.Render("~/Content/fitmax/js/parallax.min.js")
    @System.Web.Optimization.Scripts.Render("~/Content/fitmax/js/scripts.js")

    @RenderSection("scripts", required: false)
</body>
</html>