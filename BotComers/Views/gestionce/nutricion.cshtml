        function ddlPaginas_uspListarAgendaNutricionalGeneral_Paginacion(CantidadTotalFilas, TamanioPagina) {

            var CantidadPaginas = parseInt(CantidadTotalFilas / TamanioPagina) + 1;

            var htmlOpcion = "";
            for (var i = 1; i <= CantidadPaginas; i++) {
                htmlOpcion += "<option value='" + i + "'> " + i + " </option>";
            }

            $('#ddlPaginas_uspListarAgendaNutricionalGeneral_Paginacion').html(htmlOpcion);
            $("#ddlPaginas_uspListarAgendaNutricionalGeneral_Paginacion").kendoDropDownList({
                change: function () {

                    var PageNumber = $("#ddlPaginas_uspListarAgendaNutricionalGeneral_Paginacion").data('kendoDropDownList').value();

                    uspListarAgendaNutricionalGeneral_ChanguePage(PageNumber);
                }
            });
        }

