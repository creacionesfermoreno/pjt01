        function uspValidarHorariosOcupadosCitasNutricionales() {
            document.getElementById('loadMe').style.display = 'block';
            $('button[type="button"]').attr("disabled", true);

            var FechaInicio_Filtro = kendo.toString($("#txtFechaInicioCitaClientes").data('kendoDatePicker').value(), 'MM/dd/yyyy');
            var UsuarioCreacion = $("#dllNutricionistaCitaClientes").data("kendoDropDownList").value();

            $.ajax({
                data: '{"UsuarioCreacion":"' + UsuarioCreacion + '","FechaInicio_Filtro":"' + FechaInicio_Filtro + '"}',
                type: "POST",
                url: "/gestionce/uspValidarHorariosOcupadosCitasNutricionales",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {

                    for (var ti = 1; ti <= 48; ti++) {
                        $('#divPintarHoraValorAgendaGeneral' + ti).removeClass("Hseleccionado");
                        $('#divPintarHoraValorAgendaGeneral' + ti).removeClass("Hocupado");
                        $('#divPintarHoraValorAgendaGeneral' + ti).addClass("Hdisponible");
                    }

                    if (msg != 'Ninguno') {
                        if (msg.length > 0) {
                            for (var i = 0; i < msg.length; i++) {
                                $('div[data-hora="' + msg[i].strFechaCreacion + '"]').removeClass('Hdisponible');
                                $('div[data-hora="' + msg[i].strFechaCreacion + '"]').addClass("Hocupado");
                            }
                        }
                    }


                }, complete: function () {
                    document.getElementById('loadMe').style.display = 'none';
                    $('button[type="button"]').attr("disabled", false);
                }
            });

        }

        function GuardarAgendaNutricional() {

            var TipoActividad = $("#ddlTipoActividad_IngresoMedidas").data("kendoDropDownList").value();
            var UsuarioCreacion = $("#dllNutricionistaCitaClientes").data("kendoDropDownList").value();
            if (UsuarioCreacion == 'Todos') {
                $.bootstrapGrowl("Debe seleccionar un nutricionista.", { type: 'danger', width: 'auto' });
                return;
            } if ($('#hdHoraAgendaGeneral').val() == '') {
                $.bootstrapGrowl("Debe seleccionar una hora.", { type: 'danger', width: 'auto' });
                return;
            } else {

                document.getElementById('loadMe').style.display = 'block';
                $('button[type="button"]').attr("disabled", true);

                //if ($('#hdCodigoClienteCita').val() != '0') {
                //    var CodigoSocio = $('#hdCodigoClienteCita').val();
                //} else {

                //}
                var CodigoSocio = $('#hdCodigo').val(); //ESTE CODIGO VIENE DE FICHA SALUD
                var CodigoAgenda = 0;
                var asunto = $('#txtAsuntoCitaCliente').val();

                var diaG = kendo.toString($("#txtFechaInicioCitaClientes").data('kendoDatePicker').value(), 'dd');
                var MesG = kendo.toString($("#txtFechaInicioCitaClientes").data('kendoDatePicker').value(), 'MM');
                var AnioG = kendo.toString($("#txtFechaInicioCitaClientes").data('kendoDatePicker').value(), 'yyyy');

                var fecha = AnioG + "|" + MesG + "|" + diaG + "|";

                var hi = $('#hdHoraAgendaGeneral').val() == '' ? '00|00|00' : $('#hdHoraAgendaGeneral').val();
                var HoraInicio = fecha + hi;
                var Estado = 1;

                $.ajax({
                    data: '{"Codigo":"' + CodigoAgenda +
                        '","CodigoSocio":"' + CodigoSocio + '","HoraInicio":"' + HoraInicio + '","Asunto":"' + asunto + '","Estado":"' + Estado +
                        '","UsuarioCreacion":"' + UsuarioCreacion + '","TipoActividad":"' + TipoActividad + '"}',
                    type: "POST",
                    url: "/gestionce/GuardarAgendaNutricional",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        $.bootstrapGrowl("Los datos se han agregado correctamente.", { type: 'success', width: 'auto' });
                    }, complete: function () {
                        document.getElementById('loadMe').style.display = 'none';
                        $('button[type="button"]').attr("disabled", false);
                        $('#txtAsuntoCitaCliente').val('');
                        uspListarAgendaNutricionalGeneral_Paginacion();
                        //uspValidarHorariosOcupadosCitasNutricionales();
                    }
                });
            }
        }


