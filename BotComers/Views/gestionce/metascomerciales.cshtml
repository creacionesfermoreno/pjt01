        function SumarTotales_uspListarMetasDetalle_EstadisticaVenta() {
            var Cantidad = 0;
            var lblSumTicketPromedio = 0;
            var lblSumNroClientesVendidos = 0;
            var lblSumVentaAnuales = 0;
            var lblSumCantidadAnuales = 0;
            var lblSumCantidadNuevos = 0;
            var lblSumPorcentajeEfectividad = 0;
            var lblSumCantidadEfectividad = 0;

            var lblSumCantidadRenovacionPorVendedorRepartido = 0;
            var lblSumCantidadRenovacion = 0;
            var lblSumPorcentajeRenovacion = 0;


            var lblSumPorcentajeEfectividadReinscripcion = 0;
            var lblSumCantidadCierresReinscripcion = 0;
            var lblSumCantidadVentaReinscripcion = 0;

            var lblSumVentaAmpliacion = 0;
            var lblSumCantidadAmpliacion = 0;

            $("#gridMetasEstadistica .k-grid-content tbody tr").each(function () {
                Cantidad++;
                lblSumTicketPromedio += parseFloat($(this).find("td").eq(1).html());
                lblSumNroClientesVendidos += parseInt($(this).find("td").eq(2).html());
                lblSumVentaAnuales += parseFloat($(this).find("td").eq(3).html());
                lblSumCantidadAnuales += parseInt($(this).find("td").eq(4).html());
                lblSumCantidadNuevos += parseInt($(this).find("td").eq(5).html());
                lblSumCantidadEfectividad += parseInt($(this).find("td").eq(6).html());
                lblSumPorcentajeEfectividad += parseFloat($(this).find("td").eq(7).html());

                lblSumCantidadRenovacionPorVendedorRepartido += parseFloat($(this).find("td").eq(8).html());
                lblSumCantidadRenovacion += parseInt($(this).find("td").eq(9).html());
                lblSumPorcentajeRenovacion += parseFloat($(this).find("td").eq(10).html());

                lblSumPorcentajeEfectividadReinscripcion += parseInt($(this).find("td").eq(11).html());
                lblSumCantidadCierresReinscripcion += parseInt($(this).find("td").eq(12).html());
                lblSumCantidadVentaReinscripcion += parseInt($(this).find("td").eq(13).html());

                lblSumVentaAmpliacion += parseFloat($(this).find("td").eq(14).html());
                lblSumCantidadAmpliacion += parseInt($(this).find("td").eq(15).html());

            })

            $('#lblSumTicketPromedio').html((lblSumTicketPromedio.toFixed(2) / Cantidad).toFixed(2));
            $('#lblSumNroClientesVendidos').html(lblSumNroClientesVendidos);
            $('#lblSumVentaAnuales').html(lblSumVentaAnuales.toFixed(2));
            $('#lblSumCantidadAnuales').html(lblSumCantidadAnuales);
            $('#lblSumPorcentajeEfectividad').html((lblSumPorcentajeEfectividad.toFixed(2) / Cantidad).toFixed(2));
            $('#lblSumCantidadNuevos').html(lblSumCantidadNuevos);
            $('#lblSumCantidadEfectividad').html(lblSumCantidadEfectividad);
            $('#lblSumPorcentajeRenovacion').html((lblSumPorcentajeRenovacion / Cantidad).toFixed(0));
            $('#lblSumCantidadRenovacion').html(lblSumCantidadRenovacion);

            $('#lblSumCantidadCierresReinscripcion').html(lblSumPorcentajeEfectividadReinscripcion.toFixed(0));
            $('#lblSumCantidadVentaReinscripcion').html(lblSumCantidadCierresReinscripcion.toFixed(0));
            $('#lblSumPorcentajeEfectividadReinscripcion').html(lblSumCantidadVentaReinscripcion.toFixed(0));


