        function CalcularMontosTitulos(meta, bono, fechainicio, fechafin, venta) {

            $('#lblTitulo_Meta').html(parseFloat(meta).toFixed(2));
            $('#lblTitulo_Bono').html('S/. ' + parseFloat(bono).toFixed(2));
            $('#lblTitulo_VentaProduccionHastaHoy').html(parseFloat(venta).toFixed(2));
            $('#lblTitulo_VentaFaltante').html(parseFloat(parseFloat(meta).toFixed(2) - parseFloat(venta).toFixed(2)).toFixed(2));
            var porcenjateAvanzado = (parseFloat(venta).toFixed(2) * 100) / parseFloat(meta).toFixed(2);
            $('#lblTitulo_PorcentajeVentas').html('% ' + parseFloat(porcenjateAvanzado).toFixed(2));
            $('#lblTitulo_PorcentajeVentaFaltante').html('% ' + parseFloat(100 - parseFloat(porcenjateAvanzado).toFixed(2)).toFixed(2));

            var fechaFinMes = kendo.toString(kendo.parseDate(fechafin, 'MM'), 'MM');
            var fechaFinAnio = kendo.toString(kendo.parseDate(fechafin, 'yyyy-MM-dd'), 'dd/MM/yyyy').split('/')[2];
            var hoy = new Date();
            var mm = hoy.getMonth() + 1;
            var yyyy = hoy.getFullYear();

            //if (mm > 0 && mm <= 9) {
            //    var mes = 0 + '' + mm;
            //}

            if (fechaFinMes == mm && fechaFinAnio == yyyy) {
                var f = new Date();
                $('#lblTitulo_FechaHoy').html(f.getDate() + "/" + (f.getMonth() + 1) + "/" + f.getFullYear());

                switch (f.getDay()) {
                    case 1:
                        $('#lblTitulo_DiaSemanaHoy').html('Lunes');
                        break;
                    case 2:
                        $('#lblTitulo_DiaSemanaHoy').html('Martes');
                        break;
                    case 3:
                        $('#lblTitulo_DiaSemanaHoy').html('Miercoles');
                        break;
                    case 4:
                        $('#lblTitulo_DiaSemanaHoy').html('Jueves');
                        break;
                    case 5:
                        $('#lblTitulo_DiaSemanaHoy').html('Viernes');
                        break;
                    case 6:
                        $('#lblTitulo_DiaSemanaHoy').html('Sabado');
                        break;
                    case 0:
                        $('#lblTitulo_DiaSemanaHoy').html('Domingo');
                        break;

                }

                var diaFin = kendo.toString(kendo.parseDate(fechafin, 'yyyy-MM-dd'), 'dd/MM/yyyy').split('/')[0];
                var diasFaltantes = parseInt(diaFin) - parseInt(f.getDate());
                $('#lblTitulo_DiasFaltarterminarMes').html(diasFaltantes);
            } else {
                $('#lblTitulo_DiasFaltarterminarMes').html("");
                $('#lblTitulo_DiaSemanaHoy').html("");
                $('#lblTitulo_FechaHoy').html("");
            }

        }

