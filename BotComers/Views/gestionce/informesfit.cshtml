        function ddlListaPaginas(CantidadTotalFilas, TamanioPagina) {
            var CantidadPaginas = parseInt(CantidadTotalFilas / TamanioPagina) + 1;
            var htmlOpcion = "";
            for (var i = 1; i <= CantidadPaginas; i++) {
                htmlOpcion += "<option value='" + i + "'> " + i + " </option>";
            }

            $('#ddlPaginacionCLientes').html(htmlOpcion);
            $("#ddlPaginacionCLientes").kendoDropDownList({
                change: function () {

                    var ckH = $('#ckHombres').is(':checked');
                    var ckM = $('#ckMujeres').is(':checked');
                    var Sexo = '';
                    if (ckH == true && ckM == true) {
                        Sexo = 'MF';
                    } else if (ckH == true) {
                        Sexo = 'M';
                    } else if (ckM == true) {
                        Sexo = 'F';
                    } else {
                        Sexo = 'MF';
                    }

                    var EdadRango = $('#hdSlinderPrimero').val();
                    if (EdadRango == '') {
                        var EdadRango = '0,100';
                    }
                    var EdadRango1 = EdadRango.split(',')[0];
                    var EdadRango2 = EdadRango.split(',')[1];

                    var EstadoAsistencia = "";
                    $('input[name="orderBoxNew[]"]:checked').each(function () {
                        EstadoAsistencia += $(this).val();
                    });

                    var ckD = $('#ckDeudores').is(':checked');
                    var ckND = $('#ckNoDeudores').is(':checked');
                    var EstadoDeuda = '';
                    if (ckD == true && ckND == true) {
                        EstadoDeuda = '3';
                    } else if (ckD == true) {
                        EstadoDeuda = '1';
                    } else if (ckND == true) {
                        EstadoDeuda = '2';
                    } else {
                        EstadoDeuda = '3';
                    }
                    var CodDepartamento = $('#hdDepartamentoFiltro').val();
                    var CodProvincia = $('#hdProvinciaFiltro').val();
                    var CodDistrito = $('#hdUbicacionesFiltro').val();
                    var Ubicaciones = 0;
                    var TipoUbicaciones = 1;
                    if (CodDistrito != '') {
                        Ubicaciones = CodDistrito.substr(-20, 9);;
                        TipoUbicaciones = 4;
                    } else if (CodProvincia != '') {
                        Ubicaciones = CodProvincia.substr(-20, 7);
                        TipoUbicaciones = 3;
                    } else if (CodDepartamento != '') {
                        Ubicaciones = CodDepartamento.substr(-20, 5);
                        TipoUbicaciones = 2;
                    } else {
                        Ubicaciones = 0;
                        TipoUbicaciones = 1;
                    }
                    var CodigoTiempoMenbresia = $('#hdddlTiempoMembresiaPaqueteBuscador').val() == "" ? 0 : $('#hdddlTiempoMembresiaPaqueteBuscador').val();
                    var valorNombre = $('#txtBuscador').val();
                    var PageNumber = $("#ddlPaginacionCLientes").data('kendoDropDownList').value();
                    var TipoCliente = "";
                    $('input[name="orderBox[]"]:checked').each(function () {
                        TipoCliente += $(this).val();
                    });
                    var FechaInicio = kendo.toString($("#txtFechaInicioFiltro").data('kendoDatePicker').value(), 'MM/dd/yyyy');
                    var FechaFin = kendo.toString($("#txtFechaFinFiltro").data('kendoDatePicker').value(), 'MM/dd/yyyy');
                    var VendedorFiltro = $("#ddlVendedorFiltro").data("kendoDropDownList").value();

                    if (TipoCliente == 5) { //TODOS CLIENTES
                        uspListarClientesPorTodos_ChanguePage(CodigoTiempoMenbresia, Sexo, EdadRango1, EdadRango2, EstadoDeuda, EstadoAsistencia, Ubicaciones, VendedorFiltro,
                            valorNombre, valorNombre, 0, valorNombre, valorNombre, valorNombre, FechaInicio, FechaFin, 0, PageNumber);
                    } else if (TipoCliente == 2) { //CLIENTES ACTIVOS
                        uspListarClientesActivos_ChanguePage(CodigoTiempoMenbresia, Sexo, EdadRango1, EdadRango2, EstadoDeuda, EstadoAsistencia, Ubicaciones, VendedorFiltro,
                            valorNombre, valorNombre, 0, valorNombre, valorNombre, valorNombre, FechaInicio, FechaFin, PageNumber);
                    } else if (TipoCliente == 3) { //CLIENTES INACTIVOS
                        uspListarClientesInactivos_ChanguePage(CodigoTiempoMenbresia, Sexo, EdadRango1, EdadRango2, EstadoDeuda, EstadoAsistencia, Ubicaciones, VendedorFiltro,
                            valorNombre, valorNombre, 0, valorNombre, valorNombre, valorNombre, FechaInicio, FechaFin, 0, PageNumber);
                    } else if (TipoCliente == 4) { //CLIENTES POR VENCER
                        uspListarClientesPorVencer_ChanguePage(CodigoTiempoMenbresia, Sexo, EdadRango1, EdadRango2, EstadoDeuda, EstadoAsistencia, Ubicaciones, VendedorFiltro,
                            valorNombre, valorNombre, 0, valorNombre, valorNombre, valorNombre, FechaInicio, FechaFin, 0, PageNumber);
                    }


                }
            });
        }

