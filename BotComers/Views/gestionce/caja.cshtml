    //CAJA APERTURA / CIERRE

    function uspRegistrarAbrirCaja() {

        var Codigo = 0;
        var MontoApertura = $('#txtMontoApertura').val();
        var Observacion = $('#txtComentariosAjustesCaja').val();
        var TipoApertura = 0;
        var Accion = "N";


        var RAcepto = $('#RAcepto').is(':checked');
        var RAceptoConAjustes = $('#RAceptoConAjustes').is(':checked');
        var RNoAcepto = $('#RNoAcepto').is(':checked');
        if (RAcepto == true) {
            TipoApertura = 1;
        } else if (RAceptoConAjustes == true) {
            TipoApertura = 2;

            if (Observacion == '') {
                $.bootstrapGrowl("Falta ingresar el motivo del ajuste.", { type: 'danger', width: 'auto' });
                return;
            }

        } else if (RNoAcepto == true) {
            TipoApertura = 3;
        }

        $.ajax({
            data: '{"Codigo":"' + Codigo + '","MontoApertura":"' + MontoApertura +
                '","Observacion":"' + Observacion + '","TipoApertura":"' + TipoApertura + '","Accion":"' + Accion + '"}',
            type: "POST",
            url: "/gestionce/uspRegistrarAbrirCaja",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (msg) {
                $.bootstrapGrowl("la caja se aperturo correctamente.", { type: 'success', width: 'auto' });
                $('#myModalAbrirCaja').hide('fast');
                uspInformacionGeneralAbrirCaja();

            }
        });
