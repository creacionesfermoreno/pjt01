            dataType: "json",
            success: function (msg) {
                //alert(msg.Paging.TotalRecord);
                //alert(msg.Paging.PageRecords);
                var control = new Array();
                for (var i = 0; i < msg.List.length; i++) {
                    control.push('<tr class="border-bottom border-200">');
                    control.push('<td class="align-middle text-center fw-semi-bold">' + msg.List[i].FechaEmision_Texto + '</td>');
                    control.push('<td class="align-middle text-center fw-semi-bold">' + msg.List[i].NombresCliente + '</td>');
