                    control.push('<td class="align-middle text-center fw-semi-bold">' + msg.List[i].UsuarioCreacion + '</td>');
                    control.push('<td class="align-middle text-center fw-semi-bold">' + msg.List[i].SubTotal + '</td>');
                    control.push('<td class="align-middle text-center fw-semi-bold">' + msg.List[i].IGV + '</td>');
                    control.push('<td class="align-middle text-center fw-semi-bold">' + msg.List[i].Total + '</td>');
                    control.push('<td class="align-middle text-center fw-semi-bold">' + msg.List[i].DesEstado + '</td>');
                    if (msg.List[i].UrlPDF == '') {
                        control.push('<td class="align-middle text-center fw-semi-bold"></td>');
                    } else {
                        control.push('<td class="align-middle text-center fw-semi-bold"><i class="fa-solid fa-file-arrow-down"></i>&nbsp;<a href="' + msg.List[i].UrlPDF + '" target="_blank" style="color:#000;">Descargar</a></td>');
                    }

                    control.push('</tr>');
                }
                $('#gridComprobantes').html(control.join(' '));
                $('#lblCantidadComprobantes').html('Total comprobantes: ' + msg.Paging.TotalRecord);
                ddlListaPaginas_ecommerce_uspListarComprobante_Paginacion(msg.Paging.TotalRecord, msg.Paging.PageRecords);

            }, complete: function () {

                document.getElementById('loadMe').style.display = 'none';

            }
        });

