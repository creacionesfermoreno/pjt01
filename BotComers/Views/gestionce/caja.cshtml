    function uspTotalPagosVentasRangoFechas_Appsfit_Diarios() {
        //4 Servicios
        document.getElementById('loadMe').style.display = 'block';

        var Fecha = kendo.toString($("#txtFechaVentasHechaTotales").data('kendoDatePicker').value(), 'MM/dd/yyyy');
        var FechaFin = kendo.toString($("#txtFechaVentaFinTotales").data('kendoDatePicker').value(), 'MM/dd/yyyy');
        var Vendedor = $('#hdCountersTotales').val() == 'Todos Counter' ? '' : $('#hdCountersTotales').val();
        var Turno = $('#hdTurnosEmpresaTotales').val() == '' ? '0' : $('#hdTurnosEmpresaTotales').val();
        var CodTiempoPaquete = $("#ddlTiempoMembresiaPaqueteTotales").data('kendoDropDownList').value() == '' ? '0' : $("#ddlTiempoMembresiaPaqueteTotales").data('kendoDropDownList').value();
        var AsesorComercial = $('#hdAsesoresComercialesTotales').val() == 'Todos Vendedores' ? '' : $('#hdAsesoresComercialesTotales').val();
        var TipoIngreso = $("#dllTipoIngresoTotales").data('kendoDropDownList').value();
        var Tipo = 4;

        $.ajax({
            data: '{"Fecha":"' + Fecha + '","FechaFin":"' + FechaFin + '","Vendedor":"' + Vendedor + '","Turno":"' + Turno + '","CodTiempoPaquete":"' + CodTiempoPaquete + '","AsesorComercial":"' + AsesorComercial + '","TipoIngreso":"' + TipoIngreso + '","Tipo":"' + Tipo + '"}',
            type: "POST",
            url: "/gestionce/uspTotalPagosVentasRangoFechas_Appsfit",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (msg) {

                limpiarTotalesServicios('Diario');
                limpiarTotalesServicios('Servicios');

                if (msg.length > 0) {
                    //4 SERVICIOS DIARIOS
                    var MontoTotal = 0;
                    for (var i = 0; i < msg.length; i++) {
                        if (msg[i].Tipo == 4) {

                            MontoTotal = MontoTotal + msg[i].MontoTotalSalida;
                            if (msg[i].FormaPago == 1) {
                                $('#lblMontoEfectivoServicios').html(msg[i].MontoTotalSalida);
                                $('#lblDiario_FormaPago_1').html(msg[i].MontoTotalSalida);
                            }

                            if (msg[i].FormaPago == 2) {
                                $('#lblMontoDebitoServicios').html(msg[i].MontoTotalSalida);
                                $('#lblDiario_FormaPago_2').html(msg[i].MontoTotalSalida);
                            }

                            if (msg[i].FormaPago == 3) {
                                $('#lblMontoCreditoServicios').html(msg[i].MontoTotalSalida);
                                $('#lblDiario_FormaPago_3').html(msg[i].MontoTotalSalida);
                            }

                            if (msg[i].FormaPago == 4) {
                                $('#lblMontoDepositoServicios').html(msg[i].MontoTotalSalida);
                                $('#lblDiario_FormaPago_4').html(msg[i].MontoTotalSalida);
                            }

                            if (msg[i].FormaPago == 5) {
                                $('#lblMontoWebServicios').html(msg[i].MontoTotalSalida);
                                $('#lblDiario_FormaPago_5').html(msg[i].MontoTotalSalida);
                            }

                        }
                    }
                    $('#lblTotalVentaServicios').html(MontoTotal.toFixed(2));
                    $('#lblDiario_IngresoTotal').html(MontoTotal.toFixed(2));

                }

            }, complete: function () {
                document.getElementById('loadMe').style.display = 'none';

                CentroEntrenamiento_uspTotalPagosProductosRangoFechas(2, 'Productos'); //2 JUGUERIA
                CentroEntrenamiento_uspTotalPagosProductosRangoFechas(1, 'Suplementos'); //1 SUPLEMENTOS
                CentroEntrenamiento_uspTotalPagosProductosRangoFechas(3, 'Ropas');
                CentroEntrenamiento_uspTotalPagosProductosRangoFechas(4, 'Accesorios');
                uspReporteEgresoRangoFechas_Paginacion();
                TotalesTodosVentas();
            }
        });
    }

