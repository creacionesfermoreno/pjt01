                if (msg?.Status == 0) {
                    document.getElementById("embed_v_render").src = msg?.Message1;
                    document.getElementById("name_pdf_").value = msg?.Message2;
                } else {
                    $.bootstrapGrowl(msg?.Message1, { type: 'danger', width: 'auto' });
