        function uspEstadisticaDashboar() {

            var FechaInicio = kendo.toString($("#txtFechaInicio").data('kendoDatePicker').value(), 'MM/dd/yyyy');
            var FechaFinal = kendo.toString($("#txtFechaFin").data('kendoDatePicker').value(), 'MM/dd/yyyy');
            $('button[type="button"]').attr("disabled", true);

            document.getElementById('loadMe').style.display = 'block';

            $.ajax({
                data: '{"FechaInicio":"' + FechaInicio + '","FechaFinal":"' + FechaFinal + '"}',
                type: "POST",
                url: "/gestionce/uspEstadisticaDashboar",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                    $('button[type="button"]').attr("disabled", false);
                    var lbldashboard_ventamensual = parseFloat(data.dashboar_TotalVentaMesMembresias) +
                        parseFloat(data.dashboar_TotalVentaMesNutricion) +
                        parseFloat(data.dashboar_TotalVentaMesPersonalizado);
                    //parseFloat(data.dashboar_TotalVentaMesDiario) +
                    //parseFloat(data.dashboar_TotalVentaMesJugueria) +
                    //parseFloat(data.dashboar_TotalVentaMesSuplementos) +
                    //parseFloat(data.dashboar_TotalVentaMesRopas);

                    var lbldashboard_ventahoy = parseFloat(data.dashboar_TotalVentaHoyMembresias) +
                        parseFloat(data.dashboar_TotalVentaHoyNutricion) +
                        parseFloat(data.dashboar_TotalVentaHoyPersonalizado);
                    //parseFloat(data.dashboar_TotalVentaHoyDiario) +
                    //parseFloat(data.dashboar_TotalVentaHoyJugueria) +
                    //parseFloat(data.dashboar_TotalVentaHoySuplementos) +
                    //parseFloat(data.dashboar_TotalVentaHoyRopas);

                    $('#lbldashboard_ventamensual').html(lbldashboard_ventamensual.toFixed(2));
                    $('#lbldashboard_ventahoy').html(lbldashboard_ventahoy.toFixed(2));
                    $('#lbldashboard_porcobrar').html(parseFloat(data.dashboar_DebeMembresia).toFixed(2));
                    $('#lbldashboard_gastos').html(parseFloat(data.dashboar_TotalGasto).toFixed(2));

                    $('#lbldashboard_clientesactivos').html(parseInt(data.dashboar_CantidadActivos));
                    $('#lbldashboard_porvencer').html(parseInt(data.dashboar_CantidadPorVencer));
                    $('#lbldashboard_renovados').html(parseInt(data.dashboar_CantidadClientesRenovaron));

                    var lbldashboard_porcentajeretencion = (data.dashboar_CantidadClientesRenovaron / data.dashboar_CantidadPorVencer) * 100;
                    $('#lbldashboard_porcentajeretencion').html(parseFloat(lbldashboard_porcentajeretencion).toFixed(2));

                    var lbldashboard_prospectos = parseInt(data.dashboar_CantidadLlamadaEntrante) +
                        parseInt(data.dashboar_CantidadReferido) +
                        parseInt(data.dashboar_CantidadNuevos) +
                        parseInt(data.dashboar_CantidadInvitados) +
                        parseInt(data.dashboar_CantidadWeb);

                    $('#lbldashboard_inactivos').html(parseInt(data.dashboar_CantidadClientesTotalInactivos));
                    $('#lbldashboard_vencidosmes').html(parseInt(data.dashboar_CantidadClientesVencidosMes));
                    $('#lbldashboard_reinscritos').html(parseInt(data.dashboar_CantidadClientesReinscritos));
                    $('#lbldashboard_aunnoinicia').html(parseInt(data.dashboar_CantidadClientesPorIniciar));
                    $('#lbldashboard_traspasos').html(parseInt(data.dashboar_CantidadClientesTotalTraspasos));

                    $('#lbldashboard_prospectos').html(lbldashboard_prospectos);
                    $('#lbldashboard_nuevosclientes').html(parseInt(data.dashboar_CantidadClientesNuevos));

                }, complete: function () {
                    $('button[type="button"]').attr("disabled", false);
                }, complete: function () {
                    document.getElementById('loadMe').style.display = 'none';
                }
            });

        }

        //ESTADISTICAS VENTAS

        function uspEstadisticaVentasPorEvolucionTicketPromedio_Ventas() {

            var FechaInicio = kendo.toString($("#txtFechaInicio_Ventas").data('kendoDatePicker').value(), 'MM/dd/yyyy');
            var FechaFin = kendo.toString($("#txtFechaFin_Ventas").data('kendoDatePicker').value(), 'MM/dd/yyyy');
            $('button[type="button"]').attr("disabled", true);

            document.getElementById('loadMe').style.display = 'block';

            var mes = [];
            var ventamembresia = [];

            $.ajax({
                data: '{"FechaInicio":"' + FechaInicio + '","FechaFin":"' + FechaFin + '"}',
                type: "POST",
                url: "/gestionce/uspEstadisticaVentasPorEvolucionTicketPromedio_Ventas",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {

                    for (var i in msg) {
                        mes.push(msg[i].MesDescripcion);
                        ventamembresia.push(msg[i].TotalNeto);
                    }
                    $('#floarChart_uspEstadisticaVentasPorEvolucionTicketPromedio_Ventas').empty();
                    var ctx = document.getElementById('floarChart_uspEstadisticaVentasPorEvolucionTicketPromedio_Ventas');
                    var myChart = new Chart(ctx, {
                        type: 'bar',
                        data: {
                            labels: mes,
                            datasets: [{
                                label: 'Meses',
                                data: ventamembresia,
                                backgroundColor: [
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)'
                                ],
                                borderColor: [
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)',
                                    'rgb(255, 28, 6)'
                                ],
                                borderWidth: 1
                            }]
                        },
                        options: {
                            title: {
                                display: true,
                                text: 'VENTAS POR MESES'
                            },
                            scales: {
                                yAxes: [{
                                    ticks: {
                                        beginAtZero: true
                                    }
                                }]
                            }
                        }
                    });

                }, complete: function () {
                    mes = [];
                    ventamembresia = [];
                    $('button[type="button"]').attr("disabled", false);
                    uspEstadisticaVentasPorTiempoMembresia_Ventas();
                }
            });

        }

        function uspEstadisticaVentasPorTiempoMembresia_Ventas() {

            var FechaInicio = kendo.toString($("#txtFechaInicio_Ventas").data('kendoDatePicker').value(), 'MM/dd/yyyy');
