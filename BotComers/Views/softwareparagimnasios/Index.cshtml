                document.getElementById("js-validacionregistro").style.display = 'block';
                $('#txtform_nameduenio').select();
                return;
            } else if (celular == '') {
                $('#js-validacionregistro').html('Falta ingresar su nro celular.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('#txtform_whatsapp').select();
                return;
            } else if (asunto == '') {
                $('#js-validacionregistro').html('Falta ingresar el asunto.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('#txtform_asunto').select();
                return;
            }

            document.getElementById("js-validacionregistro").style.display = 'none';

            localStorage.setItem('correo', correo);
            localStorage.setItem('nombreempresa', nombreempresa);
            localStorage.setItem('situacionempresa', situacionempresa);
            localStorage.setItem('tipoempresa', tipoempresa);
            localStorage.setItem('nroubicaciones', nroubicaciones);
            localStorage.setItem('nombregerente', nombregerente);
            localStorage.setItem('asunto', asunto);

            var entidad = {};

            entidad.correo = correo;
            entidad.nombreempresa = nombreempresa;
            entidad.situacionempresa = situacionempresa;
            entidad.tipoempresa = tipoempresa;
            entidad.nroubicaciones = nroubicaciones;
            entidad.nombregerente = nombregerente;
            entidad.celular = celular;
            entidad.asunto = asunto;

            $('button[type="button"]').attr("disabled", true);
            document.getElementById('loadMe').style.display = 'block';

            var metodoCorrecto = function (msg) {

                if (msg == '') {
                    $('button[type="button"]').attr("disabled", false);
                    document.getElementById('loadMe').style.display = 'none';
                    var Link = 'https://appsfit.net/softwareparagimnasios/leadsappsfitmuchasgracias';
                    window.open(Link, '_self');
                }
                else {
                    $('button[type="button"]').attr("disabled", false);
                    document.getElementById('loadMe').style.display = 'none';
                    $('#js-validacionregistro').html(msg);
                    document.getElementById("js-validacionregistro").style.display = 'block';
                }

            };
            var metodoError = function (msg) {
                alert(msg);
            };
            var request = {
                request: entidad
            };
            LlamarAJAX("/softwareparagimnasios/EnviarCorreoLeadsappsfit", request, metodoCorrecto, metodoError);


        }

