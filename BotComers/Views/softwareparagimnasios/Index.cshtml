            if (correo == '') {
                $('#js-validacionregistro').html('Falta ingresar el correo de la empresa.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('#txtform_correo').select();
                return;
            } else if (nombreempresa == '') {
                $('#js-validacionregistro').html('Falta ingresar el nombre de la empresa.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('#txtform_nombreempresa').select();
                return;
            } else if (situacionempresa == 'Seleccione por favor') {
                $('#js-validacionregistro').html('Falta seleccionar la situaci&oacute;n de su empresa.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('select[id="ddlform_select_situacionempresa"] option:selected').focus();
                return;
            } else if (tipoempresa == 'Seleccione por favor') {
                $('#js-validacionregistro').html('Falta seleccionar el tipo de su empresa.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('select[id="ddlform_select_tipoempresa"] option:selected').focus();
                return;
            } else if (nroubicaciones == 'Seleccione por favor') {
                $('#js-validacionregistro').html('Falta seleccionar el nro de ubicaciones de su empresa.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('select[id="ddlform_select_nroubicaciones"] option:selected').focus();
                return;
            } else if (nombregerente == '') {
