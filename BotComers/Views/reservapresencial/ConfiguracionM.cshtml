        var guardarlogo = function () {

            var file = $("#buscarImagen").get(0).files;

            var detalle = $("#txtDetalle").val();
            var height = $("#hdheight_imagen").val();
            var width = $("#hdwidth_imagen").val();

            if (detalle == '') {
                $.bootstrapGrowl("Falta seleccionar una imagen.", { type: 'danger', width: 'auto' });
                return false;
            } else if (parseInt(height) > 1200) {
                $.bootstrapGrowl("El alto de la imagen no puede ser mayor 1200.", { type: 'danger', width: 'auto' });
                return false;
            } else if (parseInt(width) > 1200) {
                $.bootstrapGrowl("El ancho de la imagen no puede ser mayor 1200.", { type: 'danger', width: 'auto' });
                return false;
            } else if (file == undefined || file == null) {
                $.bootstrapGrowl("Falta seleccionar una imagen.", { type: 'danger', width: 'auto' });
                return false;
            }
            document.getElementById('loadMe').style.display = 'block';

            var data = new FormData;
            data.append("file", file[0]);

            $.ajax({
                type: "Post",
                url: "/reservapresencial/GuardarConfiguracion_logo",
                data: data,
                contentType: false,
                processData: false,
                success: function (response) {
                    $.bootstrapGrowl("El logo ha sido guardado correctamente.", { type: 'success', width: 'auto' });
                }, complete: function () {
                    document.getElementById('loadMe').style.display = 'none';
                }
            })

        }

