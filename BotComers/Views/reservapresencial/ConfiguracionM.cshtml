                }
            });

            $('#buscarImagen').change(function () {
                var File = this.files;
                if (File && File[0]) {
                    ReadImage(File[0]);
                }
            });

            var todayDate = new Date();
            $("#txtClase_HoraInicio").kendoTimePicker({
                dateInput: true,
                format: "hh:mm tt",
                interval: 60,
                min: new Date(new Date().setHours(5, 0, 0, 0)),
                max: new Date(new Date().setHours(23, 0, 0, 0)),
                change: function () {
                    var value = this.value();
                    if (value == null) {
                        $('#txtClase_HoraInicio').parent().parent().addClass('borderValidar');
                    }
                    else {
                        $('#txtClase_HoraInicio').parent().parent().removeClass('borderValidar');
                        var timepicker = $("#txtClase_HoraFin").data("kendoTimePicker");
                        timepicker.setOptions({ min: value });
                        timepicker.value(null);
                    }
                }
            });
            $("#txtClase_HoraFin").kendoTimePicker({
                dateInput: true,
                format: "hh:mm tt",
                interval: 60,
                min: new Date(new Date().setHours(6, 0, 0, 0)),
                max: new Date(new Date().setHours(23, 45, 0, 0)),
                change: function () {
                    var value = this.value();
                    if (value == null) {
                        $('#txtClase_HoraFin').parent().parent().addClass('borderValidar');
                    }
                    else {
                        $('#txtClase_HoraFin').parent().parent().removeClass('borderValidar');
                    }
                }
            });

            activarControlesHora();
            
            uspListarDisciplinaSala_Presencial();
            
