        var ReadImage = function (file) {

            var reader = new FileReader;
            var image = new Image;

            reader.readAsDataURL(file);
            reader.onload = function (_file) {

                image.src = _file.target.result;
                image.onload = function () {
                    var height = this.height;
                    var width = this.width;
                    var type = file.type;
                    var size = ~~(file.size / 1024) + "KB";

                    $("#imgLogoPlataforma").attr('src', _file.target.result);
                    $("#txtDetalle").val("Size:" + size + ", " + height + " X " + width + ", " + type + "");
                    $("#hdheight_imagen").val(height);
                    $("#hdwidth_imagen").val(width);
                    if (parseInt(height) > 1200) {
                        $.bootstrapGrowl("El alto de la imagen no puede ser mayor 1200, seleccione otra imagen.", { type: 'danger', width: 'auto' });
                    } else if (parseInt(width) > 1200) {
                        $.bootstrapGrowl("El ancho de la imagen no puede ser mayor 1200, seleccione otra imagen.", { type: 'danger', width: 'auto' });
                    }
                }

            }
        }

