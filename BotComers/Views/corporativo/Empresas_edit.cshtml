        var ReadImage = function (file) {

            var reader = new FileReader;
            var image = new Image;

            reader.readAsDataURL(file);
            reader.onload = function (_file) {

                image.src = _file.target.result;
                image.onload = function () {

                    var height = this.height;
                    var width = this.width;
                    var type = file.type;
                    var size = ~~(file.size / 1024) + "KB";

                    $("#imgVistaImagen").attr('src', _file.target.result);
                    $("#txtDetalle").val("Size:" + size + ", " + height + " X " + width + ", " + type + "");
                    $("#hdheight_imagen").val(height);
                    $("#hdwidth_imagen").val(width);
                    if (parseInt(height) > 2100) {

                        $.bootstrapGrowl("El alto de la imagen no puede ser mayor 2100, seleccione otra imagen.", { type: 'danger', width: 'auto' });
                    } else if (parseInt(width) > 2100) {

                        $.bootstrapGrowl("El ancho de la imagen no puede ser mayor 2100, seleccione otra imagen.", { type: 'danger', width: 'auto' });
                    }

                }

            }
        }

        var guardarlogo = function () {

            var file = $("#buscarImagen").get(0).files;

            var detalle = $("#txtDetalle").val();
            var height = $("#hdheight_imagen").val();
            var width = $("#hdwidth_imagen").val();

            if (detalle == '') {
                $.bootstrapGrowl("Falta seleccionar una imagen.", { type: 'danger', width: 'auto' });
                return false;
            } else if (parseInt(height) > 2100) {
                $.bootstrapGrowl("El alto de la imagen no puede ser mayor 2100.", { type: 'danger', width: 'auto' });
                return false;
            } else if (parseInt(width) > 2100) {
                $.bootstrapGrowl("El ancho de la imagen no puede ser mayor 2100.", { type: 'danger', width: 'auto' });
                return false;
            } else if (file == undefined || file == null) {
                $.bootstrapGrowl("Falta seleccionar una imagen.", { type: 'danger', width: 'auto' });
                return false;
            }
            document.getElementById('loadMe').style.display = 'block';

            var CodigoUnidadNegocio = $("#txtCodigoUnidadNegocio").val();
            var CodigoSede = $("#txtCodigoSede").val();
            var data = new FormData;
            data.append("file", file[0]);
            data.append("CodigoUnidadNegocio", CodigoUnidadNegocio);
            data.append("CodigoSede", CodigoSede);

            $.ajax({
                type: "Post",
                url: "/corporativo/GuardarConfiguracion_logo",
                data: data,
                contentType: false,
                processData: false,
                success: function (response) {
                    $.bootstrapGrowl("El logo ha sido guardada correctamente.", { type: 'success', width: 'auto' });
                }, complete: function () {

                    document.getElementById('loadMe').style.display = 'none';
                }
            })

        }


