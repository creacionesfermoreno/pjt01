        $(document).ready(function () {

            $('#btnFiltroBusqueda').click(function () {
                uspListarConfiguracion_apfitness_Paginacion();
            });

            uspListarConfiguracion_apfitness_Paginacion();

        });

        function uspListarConfiguracion_apfitness_Paginacion() {
            var filtro = $("#txtBuscadorGym").val();
            var Estado = $('input:radio[name=groupRadiosEstadoBuscador]:checked').val();
            document.getElementById('loadMe').style.display = 'block';

            $.ajax({
                data: '{"estado":"' + Estado + '","PageNumber":"' + 1 + '","filtro":"' + filtro + '"}',
                type: "POST",
                url: "/corporativo/uspListarConfiguracion_apfitness_Paginacion",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    //options.success(msg);

                    var control = new Array();
                    for (var i = 0; i < msg.length; i++) {
                        control.push('<tr class="border-bottom border-200">');

                        control.push("<td class='align-middle white-space-nowrap'><a id='btnVerDatosUnidadNegocio_" + msg[i].CodigoUnidadNegocio + msg[i].Codigo + " style='display:none;' href='/corporativo/Empresas_edit?idUN=" + msg[i].CodigoUnidadNegocio + "&idSede=" + msg[i].Codigo + "' ><button type='button' class='btn btn-sm btn-light' >Editar</button></a></td>");
                        control.push('<td class="align-middle white-space-nowrap"><center><div style="background-color:' + msg[i].DesEstado + ';width: 13px;height: 13px;border-radius:25px;"></div></center></td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].CodigoUnidadNegocio + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].Codigo + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].Pais + '</td>');
                        control.push('<td class="align-middle white-space-nowrap"><img style="height:55px;width:55px;border-radius: 50%;" src="' + msg[i].Logo + '" alt="Logo"></td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].RazonSocial + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].ContactoCobranza + '</td>');
                        control.push('<td class="align-middle white-space-nowrap"><input type="text" class="k-textbox" style="width: 220px;font-size:12px;" value="' + msg[i].Correo + '"></td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].DiaPago + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].TipoMoneda + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].MontoMensualidad + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].CantTotal + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].CantidadActivosSocios + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].EntidadBancaria + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].ResponsableCuenta + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + kendo.toString(kendo.parseDate(msg[i].FechaVencimiento, 'yyyy-MM-dd'), 'dd/MM/yyyy') + '</td>');
                        control.push('<td class="align-middle white-space-nowrap"><center><a href="' + msg[i].SubDominio + '" style="color:#000">Visitar</a></center></td>');
                        

                        control.push('</tr>');
                    }
                    $('#gvListadoGym').html(control.join(' '));


                }, complete: function () {
                    uspListarConfiguracion_apfitness_NumeroRegistros();
                    document.getElementById('loadMe').style.display = 'none';
                }
            });

        }

        function uspListarConfiguracion_apfitness_NumeroRegistros() {
            var filtro = $("#txtBuscadorGym").val();
            document.getElementById('loadMe').style.display = 'block';
            $.ajax({
                type: "POST",
                data: '{"filtro":"' + filtro + '"}',
                url: "/corporativo/uspListarConfiguracion_apfitness_NumeroRegistros",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    $('#lblCantidadTodosEmpresa').html(msg.CantTotal);
                    $('#lblCantidadActivosEmpresa').html(msg.CantidadEmpresasActivas);
                    $('#lblCantidadProspectosEmpresa').html(msg.CantidadEmpresasProspectos);
                    $('#lblCantidadPruebaEmpresa').html(msg.CantidadEmpresasPrueba);
                    $('#lblCantidadInactivosEmpresa').html(msg.CantidadEmpresasInactivas);
                    $('#lblCantidadRetiradosEmpresa').html(msg.CantidadEmpresasRetirados);

                    ddlPaginacionuspListarConfiguracion_apfitness_Paginacion(msg.CantTotal, msg.TamanioPagina);

                }, complete: function () {
                    document.getElementById('loadMe').style.display = 'none';
                }
            });
        }

        function ddlPaginacionuspListarConfiguracion_apfitness_Paginacion(CantidadTotalFilas, TamanioPagina) {
            var CantidadPaginas = parseInt(CantidadTotalFilas / TamanioPagina) + 1;
            var htmlOpcion = "";
            for (var i = 1; i <= CantidadPaginas; i++) {
                htmlOpcion += "<option value='" + i + "'> " + i + " </option>";
            }

            $('#ddlPaginacionGym').html(htmlOpcion);
            $("#ddlPaginacionGym").kendoDropDownList({
                change: function () {
                    var nroPagina = $("#ddlPaginacionGym").data("kendoDropDownList").value();
                    uspListarConfiguracion_apfitness_Paginacion_ChanguePage(nroPagina);
                }
            });
        }

        function uspListarConfiguracion_apfitness_Paginacion_ChanguePage(PageNumber) {

            var filtro = $("#txtBuscadorGym").val();
            var Estado = $('input:radio[name=groupRadiosEstadoBuscador]:checked').val();
            document.getElementById('loadMe').style.display = 'block';

            $.ajax({
                data: '{"estado":"' + Estado + '","PageNumber":"' + PageNumber + '","filtro":"' + filtro + '"}',
                type: "POST",
                url: "/corporativo/uspListarConfiguracion_apfitness_Paginacion",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    //options.success(msg);

                    var control = new Array();
                    for (var i = 0; i < msg.length; i++) {
                        control.push('<tr class="border-bottom border-200">');

                        control.push("<td class='align-middle white-space-nowrap'><a id='btnVerDatosUnidadNegocio_" + msg[i].CodigoUnidadNegocio + msg[i].Codigo + " style='display:none;' href='/corporativo/Empresas_edit?idUN=" + msg[i].CodigoUnidadNegocio + "&idSede=" + msg[i].Codigo + "' ><button type='button' class='btn btn-sm btn-light' >Editar</button></a></td>");
                        control.push('<td class="align-middle white-space-nowrap"><center><div style="background-color:' + msg[i].DesEstado + ';width: 13px;height: 13px;border-radius:25px;"></div></center></td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].CodigoUnidadNegocio + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].Codigo + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].Pais + '</td>');
                        control.push('<td class="align-middle white-space-nowrap"><img style="height:55px;width:55px;border-radius: 50%;" src="' + msg[i].Logo + '" alt="Logo"></td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].RazonSocial + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].ContactoCobranza + '</td>');
                        control.push('<td class="align-middle white-space-nowrap"><input type="text" class="k-textbox" style="width:220px;font-size:12px;" value="' + msg[i].Correo + '"></td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].DiaPago + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].TipoMoneda + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].MontoMensualidad + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].CantTotal + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].CantidadActivosSocios + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].EntidadBancaria + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + msg[i].ResponsableCuenta + '</td>');
                        control.push('<td class="align-middle white-space-nowrap">' + kendo.toString(kendo.parseDate(msg[i].FechaVencimiento, 'yyyy-MM-dd'), 'dd/MM/yyyy') + '</td>');
                        control.push('<td class="align-middle white-space-nowrap"><center><a href="' + msg[i].SubDominio + '" style="color:#000">Visitar</a></center></td>');

                        control.push('</tr>');
                    }
                    $('#gvListadoGym').html(control.join(' '));


                }, complete: function () {
                    
                    document.getElementById('loadMe').style.display = 'none';
                }
            });

         
        }


    </script>
</body>
