        function EnviarDatos() {

            var correo = $('#txtform_correo').val();
            var nombreempresa = $('#txtform_nombreempresa').val();
            var situacionempresa = $('select[id="ddlform_select_situacionempresa"] option:selected').text();
            var tipoempresa = $('select[id="ddlform_select_tipoempresa"] option:selected').text();
