            var nombregerente = $('#txtform_nameduenio').val();
            var asunto = $('#txtform_asunto').val();

            if (correo == '') {
                $('#js-validacionregistro').html('Falta ingresar el correo de la empresa.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('#txtform_correo').select();
                return;
            } else if (nombreempresa == '') {
                $('#js-validacionregistro').html('Falta ingresar el nombre de la empresa.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('#txtform_nombreempresa').select();
                return;
            } else if (situacionempresa == 'Seleccione por favor') {
                $('#js-validacionregistro').html('Falta seleccionar la situación de su empresa.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('select[id="ddlform_select_situacionempresa"] option:selected').focus();
                return;
            } else if (tipoempresa == 'Seleccione por favor') {
                $('#js-validacionregistro').html('Falta seleccionar el tipo de su empresa.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('select[id="ddlform_select_tipoempresa"] option:selected').focus();
                return;
            } else if (nroubicaciones == 'Seleccione por favor') {
                $('#js-validacionregistro').html('Falta seleccionar el nro de ubicaciones de su empresa.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('select[id="ddlform_select_nroubicaciones"] option:selected').focus();
                return;
            } else if (nombregerente == '') {
                $('#js-validacionregistro').html('Falta ingresar su nombre y apellidos.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('#txtform_nameduenio').select();
                return;
            } else if (asunto == '') {
                $('#js-validacionregistro').html('Falta ingresar el asunto.');
                document.getElementById("js-validacionregistro").style.display = 'block';
                $('#txtform_asunto').select();
                return;
            }

            document.getElementById("js-validacionregistro").style.display = 'none';

            correo = correo.replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20');
            nombreempresa = nombreempresa.replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20');
            situacionempresa = situacionempresa.replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20');
            tipoempresa = tipoempresa.replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20');
            nroubicaciones = nroubicaciones.replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20');
            nombregerente = nombregerente.replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20');
            asunto = asunto.replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20').replace(' ', '%20');

            var mensaje = '';

            mensaje = mensaje + 'Mi%20empresa%20se%20llama:%20' + nombreempresa + '%0A';
            mensaje = mensaje + 'Mi%20correo%20es:%20' + correo + '%0A';
