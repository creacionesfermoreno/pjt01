<section class="appointment top-space" id="register" style="display:@(Model.SesionFormulario_Estado ? "block" : "none")">
    <div class="container">

        <div class="row">

            <div class="col-sm-6 col-md-7">
                <div class="section-title left-aligned">
                    <h2 style="color:var(--colorPrincipal);font-weight:bold;">@Model.SesionFormulario_Titulo</h2>
                    <h3 class="subline" style="color:#636363;">@Model.SesionFormulario_Descripcion</h3>
                    <hr>
                </div>

                <img src="@Model.SesionFormulario_Imagen" class="img-responsive" alt="Yoga Pose" />
            </div>
            <!-- // end .col -->

            <div class="col-sm-6 col-md-5">

                <div class="appointment-form top-space-lg">

                    <div style="background-color:var(--colorPrincipal);border: 1px var(--colorPrincipal) solid;" class="form-header">
                        <h4 style="font-weight:bold;">@Model.SesionFormulario_Tituloformulario</h4>

                    </div>

                    <div class="form-group" style="padding-left:15px;padding-right:15px;">
                        <label style="color:#000;">Nombre completo</label>
                        <input id="txtSesionFormulario_NombreCompleto" type="text" class="form-control" style="border: 1px solid #caced1;font-size:18px;font-weight:bold;padding-left:10px;" required title="Ingrese nombres...">
                    </div>
                    <div class="form-group" style="padding-left:15px;padding-right:15px;">
                        <label style="color:#000;">¿Qué objetivo quieres lograr?</label>
                        <select id="ddlSesionFormulario_Objetivo" class="form-control" style="border: 1px solid #caced1;">
                            <option value="0">Seleccionar</option>
                            <option value="1">Bajar de peso</option>
                            <option value="2">Aumentar masa muscular</option>
                            <option value="3">Mejorar la salud</option>
                            <option value="4">Superar el stress</option>
                            <option value="5">tonificar mi cuerpo</option>
                            <option value="6">Preparación fisica</option>
                        </select>
                    </div>
                    <div class="form-group" style="padding-left:15px;padding-right:15px;">
                        <label style="color:#000;">¿En cuanto tiempo te gustaria lograrlo?</label>
                        <select id="ddlSesionFormulario_Promocion" class="form-control" style="border: 1px solid #caced1;">
                            <option value="0">Seleccionar</option>
                            @{if (Model.List_Planes != null)
                                {
                                    foreach (var itemPlanes in Model.List_Planes)
                                    {
                                        <option>@itemPlanes.Titulo</option>
                                    }
                                }
                            }
                        </select>
                    </div>
                    <input id="txtSesionFormulario_CodigoPais" type="hidden" value="@Model.SesionFormulario_CodigoPais" />
                    <input id="txtSesionFormulario_Whatsapp" type="hidden" value="@Model.SesionFormulario_Whatsapp" />
                    <div class="text-center top-space" style="padding-left:15px;padding-right:15px;">
                        <button onclick="EnviarDatos();" style="background-color:var(--colorPrincipal);color:#fff;border: 1px var(--colorPrincipal) solid;" type="submit" class="btn btn-block btn-xl" id="js-contact-btn">Enviar información</button>                        
                        <div id="js-validacion" style="color:red;display:none;"></div>
                    </div>
                    <br />

                </div>

            </div>
            <!-- // end .col -->

        </div>
        <!-- // end .row -->

    </div>
    <!-- end .container -->
</section>

