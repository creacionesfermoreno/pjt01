        //---------------------- tipo de contrato membresia ------------------------
        //registro
        function registerTC() {
            var correlativo = $('#txtCorrelativoTC').val();
            var serie = $('#txtSerieTC').val();
            var description = $('#txtDescripcionTC').val();
            var clausula = $("#txtClausula").data("kendoEditor").value();
            let data = {
                description: description,
                clausula: clausula,
                serie: serie,
                correlativo: correlativo,
            }

            if (description.trim() != '' && clausula.trim() != '' && correlativo.trim() != '' && serie.trim() != '') {
                $.ajax({
                    data: JSON.stringify(data),
                    type: "POST",
                    url: "/operacionesfit/RegistrarTipoContratos",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        if (msg.Success) {
                            $.bootstrapGrowl("Los datos se han guardado correctamente.", { type: 'success', width: 'auto' });
                            document.getElementById('loadMe').style.display = 'none';
                            document.getElementById("txtDescripcionTC").value = ''
                            $("#txtClausula").data("kendoEditor").value('')
                            getContratos()
                        } else {
                            $.bootstrapGrowl("Ocurrió un error en el proceso.", { type: 'danger', width: 'auto' });
                            document.getElementById('loadMe').style.display = 'none';
                        }

                    }
                });
            } else {
                $.bootstrapGrowl("Campos requeridos.", { type: 'danger', width: 'auto' });
            }


        }
        //list
        function getContratos() {
            $.ajax({
                type: "POST",
                url: "/operacionesfit/ListaTipoContratos",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    console.log(msg)
                    if (msg.length > 0) {
                        var html = '';
                        msg.forEach((item, index) => {
                            html += '<tr class="border-bottom border-200">';
                            html += ' <td class="align-middle white-space-nowrap">' + parseInt(index + 1) + '</td>';
                            html += '  <td class="align-middle white-space-nowrap">' + item.NroSerie + '</td>';
                            html += '  <td class="align-middle white-space-nowrap">' + item.NroCorrelativoActual + '</td>';
                            html += '  <td class="align-middle white-space-nowrap">' + item.Descripcion + '</td>';
                            html += '  <td class="align-middle white-space-nowrap">';
                            html += `  <button class="btn btn-custom-primary" onclick="editTc(${item?.Codigo})"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></button>`;
                            html += `  <button class="btn btn-danger" onclick="destroyTc(${item?.Codigo})"><i class="fa fa-trash text-white" aria-hidden="true"></i></button>`;
                            html += ' </td>';
                            html += '</tr>';
                        });
                        document.getElementById("tbodyTipoContratos").innerHTML = html

                    }
                    console.log(msg.length);
                }
            });
        }

        getContratos()

        function editTc(codigo) {
            document.getElementById("txtCodigoC").value = codigo;
            $.ajax({
                type: "POST",
                url: "/operacionesfit/GetTipoContrato",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify({ codigo: codigo }),
                dataType: "json",
                success: function (msg) {
                    if (msg) {
                        $('#txtCorrelativoTC').val(msg?.NroCorrelativoActual);
                        $('#txtSerieTC').val(msg?.NroSerie);
                        $('#txtDescripcionTC').val(msg?.Descripcion);
                        $("#txtClausula").data("kendoEditor").value(msg?.Clausula);
                        document.getElementById("btnRegisterTc").style.display = 'none';
                        document.getElementById("btnUpdateTc").style.display = 'block';
                        document.getElementById("btnCancelTc").style.display = 'block';
                    }
                }
            });
        }
        function cancelTC() {
            document.getElementById("txtCodigoC").value = '';
            document.getElementById("txtCorrelativoTC").value = '';
            document.getElementById("txtSerieTC").value = '';
            document.getElementById("txtDescripcionTC").value = '';
            $("#txtClausula").data("kendoEditor").value('');
            document.getElementById("btnRegisterTc").style.display = 'block';
            document.getElementById("btnUpdateTc").style.display = 'none';
            document.getElementById("btnCancelTc").style.display = 'none';
        }
        function updateTC() {
            var codigo = document.getElementById("txtCodigoC").value;

            var correlativo = $('#txtCorrelativoTC').val();
            var serie = $('#txtSerieTC').val();

            var description = $('#txtDescripcionTC').val();
            var clausula = $("#txtClausula").data("kendoEditor").value();

            var data = {
                codigo: codigo,
                clausula: clausula,
                description: description,
                serie: serie,
                correlativo: correlativo,
            }

            $.ajax({
                data: JSON.stringify(data),
                type: "POST",
                url: "/operacionesfit/ActualizarTipoContratos",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {

                    if (msg.Success) {
                        $.bootstrapGrowl("Los datos se han guardado correctamente.", { type: 'success', width: 'auto' });
                        document.getElementById('loadMe').style.display = 'none';
                        //document.getElementById("txtDescripcionTC").value = ''
                        //$("#txtClausula").data("kendoEditor").value('')
                        getContratos()
                    } else {
                        $.bootstrapGrowl("Ocurrió un error en el proceso.", { type: 'danger', width: 'auto' });
                        document.getElementById('loadMe').style.display = 'none';
                    }

                }
            });

        }
        function destroyTc(codigo) {
            var confirm = window.confirm("estas seguro de eliminar?");
            if (confirm) {
                $.ajax({
                    data: JSON.stringify({ codigo: codigo }),
                    type: "POST",
                    url: "/operacionesfit/EliminarTipoContratos",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {

                        if (msg.Success) {
                            $.bootstrapGrowl("Eliminado correctamente.", { type: 'success', width: 'auto' });
                            document.getElementById('loadMe').style.display = 'none';
                            //document.getElementById("txtDescripcionTC").value = ''
                            //$("#txtClausula").data("kendoEditor").value('')
                            getContratos()
                        } else {
                            $.bootstrapGrowl("Ocurrió un error en el proceso.", { type: 'danger', width: 'auto' });
                            document.getElementById('loadMe').style.display = 'none';
                        }

                    }
                });
            }


        }
