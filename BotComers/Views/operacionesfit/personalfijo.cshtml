            entidad.request.NumeroDocumento = $('#txtNroDocumentoPersonal').val();
            entidad.request.Nombres = $('#txtNombrePersonal').val();

            var EsValido = true;

            if ((entidad.request.CodigoPersonalAdministrativo.length <= 10)) {

                EsValido = false;
                return;
            }
            if (IsUndefinedOrNullOrEmpty(entidad.request.NumeroDocumento)) {
                $('#txtNroDocumentoPersonal').addClass('borderValidar');
                EsValido = false;
                return;
            }
            else {
                $('#txtNroDocumentoPersonal').removeClass('borderValidar');
            }

            if (EsValido) {
                document.getElementById('loadMe').style.display = 'block';
                $.ajax({
                    data: JSON.stringify(entidad),
                    type: "POST",
                    url: "/operacionesfit/ActivarPersonalAdministrativo",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        if (msg.Success) {
                            NuevoPersonalAdministrativo();
                            ListarPersonalAdministrativo();
                        }
                        else {
                            if (msg.MessageList.length > 0) {
                                var mensaje = msg.MessageList[0].Detalle;
