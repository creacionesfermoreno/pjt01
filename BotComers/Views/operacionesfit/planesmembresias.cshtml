        function ddlPaginacionuspListarServicios_Paginacion(CantidadTotalFilas, TamanioPagina) {
            var CantidadPaginas = parseInt(CantidadTotalFilas / TamanioPagina) + 1;
            var htmlOpcion = "";
            for (var i = 1; i <= CantidadPaginas; i++) {
                htmlOpcion += "<option value='" + i + "'> " + i + " </option>";
            }
            $('#ddlPaginacionuspListarServicios').html(htmlOpcion);
            $("#ddlPaginacionuspListarServicios").kendoDropDownList({
                change: function () {
                    var nroPagina = $("#ddlPaginacionuspListarServicios").data("kendoDropDownList").value();
                    uspListarProductoElaboradoPorFiltro_ChanguePage(nroPagina);
                }
            });
        }

        function buscarProductoElaborado(CodigoProducto) {

            $.ajax({
                data: '{"CodigoProducto":"' + CodigoProducto + '"}',
                type: "POST",
                url: "/operacionesfit/BuscarProductoElaborado",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    $('#txtCodigoServicios').val(msg.CodigoProducto);
                    $('#txtDescripcionServicios').val(msg.Descripcion);
                    $('#txtprecioServicios').val(msg.PrecioVenta);
                    $('#hdAccionServicios').val('E');
                }
            });
        }


        function validadorProductoElaborado() {
            var validator = true;
            if ($('#txtDescripcionServicios').val() == '') {
                validator = false;
                $('#txtDescripcionServicios').addClass('borderValidar');
            } else {
                $('#txtDescripcionServicios').removeClass('borderValidar');
            }
            if ($('#txtprecioServicios').val() <= 0) {
                validator = false;
                $('#txtprecioServicios').addClass('borderValidar');
            } else {
                $('#txtprecioServicios').removeClass('borderValidar');
            }
            return validator;
        }


        function GuardarProductoElaborado() {

            var CodigoProducto = $('#txtCodigoServicios').val() == '' ? '0' : $('#txtCodigoServicios').val();
            var Descripcion = $('#txtDescripcionServicios').val();
            var PrecioVenta = $('#txtprecioServicios').val();
            var CodigoCategoria = 0;
            var Accion = $('#hdAccionServicios').val();

            $.ajax({
                type: "POST",
                data: '{"CodigoProducto":"' + CodigoProducto + '","Descripcion":"' + Descripcion + '","PrecioVenta":"' + PrecioVenta + '","CodigoCategoria":"' + CodigoCategoria + '","Accion":"' + Accion + '"}',
                url: "/operacionesfit/GuardarProductoElaborados",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    if (msg > 0) {
                        if (Accion == 'N') {
                            $.bootstrapGrowl("Los datos se han guardado correctamente", { type: 'success', width: 'auto' });
                        } else {
                            $.bootstrapGrowl("Los datos se han actualizado correctamente.", { type: 'success', width: 'auto' });
                        }
                        uspListarProductoElaboradoPorFiltro_Paginacion();

                    } else {
                        $.bootstrapGrowl("Su tiempo se agoto vuelva a ingresar al sistema por favor, su ingreso solo dura 24 horas. o este paquete no se puede eliminar.", { type: 'danger', width: 'auto' });
                    }
                }, complete: function () {

                }
            });
        }

        function ListadoServiciosPorFiltro() {
            uspListarProductoElaboradoPorFiltro_Paginacion();
        }

