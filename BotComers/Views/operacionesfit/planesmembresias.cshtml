                    $('#txtCodigoPaquete').val(msg.CodigoPaquete);
                    $('#txtDescripcionPaquete').val(msg.Descripcion);


                    $('#chkshowApp').prop('checked', msg?.ShowApp);
                    $('#chksuscription').prop('checked', msg?.Suscripcion);


                    $('#txtValorPaquete').val(msg.valor);
                    $('#txtFrezenDisponiblesPaquete').val(msg.CongelamientoVigente);
                    $('#txtCostoPaquete').val(msg.Costo);
                    $('#chkEstadoPaquete').prop('checked', msg.Estado);
                    $('#chkMembresiaInterdiaria').prop('checked', msg.EstadoMembresiaInterdiaria);
                    $('#txtFechaVencimientoPaquete').data('kendoDatePicker').value(msg.FechaVencimiento);
                    $('#hdAccionCrearPaquetes').val('E');
                    $('#cboTipoPaquete').data("kendoDropDownList").value(msg.CodigoTipoPaquete);

                    document.getElementById("fileImagenPlanUrlRender").src = msg.UrlImage;

                    document.getElementById("dgestionSuscriptionxxx").style.display = "";





                    var tipo = msg.CodigoTipoPaquete;
                    if (tipo == 2) {
                        document.getElementById("trOptionMembresiaInterdiaria").style.display = '';
                    } else {
                        document.getElementById("trOptionMembresiaInterdiaria").style.display = 'none';
                    }

                    $('#ddlTiempoMembresiaPaquete').data("kendoDropDownList").value(msg.TiempoMembresia + '|' + msg.ValorDias_Tiempo);
                    var FechaVen = kendo.toString($("#txtFechaVencimientoPaquete").data('kendoDatePicker').value(), 'dd/MM/yyyy');
                    if (FechaVen == "31/12/2099") {
                        $('#txtFechaVencimientoPaquete').data("kendoDatePicker").enable(false);
                        $('#chkNuncavence').prop('checked', true);
                    } else {
                        $('#txtFechaVencimientoPaquete').data("kendoDatePicker").enable(true);
                        $('#chkNuncavence').prop('checked', false);
                    }
                    $('#txtNumeroCupo').val(msg.NroCupo);
                    $('#txtFechaInicioPaquete').data('kendoDatePicker').value(msg.FechaInicio);
                    if (msg.EstadoFecha == 1) {
                        $('#chkUsarFInicioFin').prop('checked', true);
                    } else {
                        $('#chkUsarFInicioFin').prop('checked', false);
                    }
                    $('#txtNroIngresoDia').val(msg.NroIngresoDia);
                }
            });
        }

