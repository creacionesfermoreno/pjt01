        function listaTipoPaquete() {

            var cboTipoPaquete = $("#cboTipoPaquete").kendoDropDownList({
                dataTextField: "Descripcion",
                dataValueField: "Codigo",
                dataSource: {
                    serverFiltering: false,
                    transport: {
                        read: function (options) {
                            $.ajax({
                                type: "POST",
                                url: "/operacionesfit/listaTipoPaquete",
                                contentType: "application/json; charset=utf-8",
                                dataType: "json",
                                success: function (msg) {
                                    options.success(msg);
                                }
                            });
                        }
                    }
                }, change: function () {
                    var tipo = $("#cboTipoPaquete").data("kendoDropDownList").value();
                    if (tipo == 2) {
                        document.getElementById("trOptionMembresiaInterdiaria").style.display = '';
                    } else {
                        document.getElementById("trOptionMembresiaInterdiaria").style.display = 'none';
                    }
                    $('#chkMembresiaInterdiaria').prop('checked', false);
                }
            }).data("kendoDropDownList");
        }

        function ListaTiempoMembresiaPaquete() {
            var ddlTiempoMembresiaPaquete = $("#ddlTiempoMembresiaPaquete").kendoDropDownList({
                filter: "startswith",
                optionLabel: "Seleccione",
                dataTextField: "Descripcion",
                dataValueField: "Valor", //CodigoTiempo
                dataSource: {
                    serverFiltering: true,
                    transport: {
                        read: function (options) {
                            var nombre = $('input[aria-owns="ddlTiempoMembresia_listbox"]').val() == 'undefined' ? '' : $('input[aria-owns="ddlTiempoMembresia_listbox"]').val();
                            $.ajax({
                                data: '{"nombre":"' + nombre + '"}',
                                type: "POST",
                                url: "/operacionesfit/ListaTiempoMembresia",
                                contentType: "application/json; charset=utf-8",
                                dataType: "json",
                                success: function (msg) {
                                    options.success(msg);
                                }
                            });
                        }
                    }
                }, change: function () {
                    var valor = $("#ddlTiempoMembresiaPaquete").data("kendoDropDownList").value().split('|')[1];
                    $('#txtValorPaquete').val(valor);
                }
            }).data("kendoDropDownList");
        }

        function listarddlTipoPaquete() {

            var ddlPaquete = $("#ddlTipoPaquete").kendoDropDownList({
                dataTextField: "Descripcion",
                dataValueField: "Codigo",
                dataSource: {
                    serverFiltering: true,
                    transport: {
                        read: function (options) {
                            $.ajax({
                                type: "POST",
                                url: "/operacionesfit/listarddlTipoPaquete",
                                contentType: "application/json; charset=utf-8",
                                dataType: "json",
                                success: function (msg) {
                                    options.success(msg);
                                    ListadoPaquetesPorFiltro();
                                }
                            });
                        }
                    }
                }, change: function () {
                    ListadoPaquetesPorFiltro();
                }
            }).data("kendoDropDownList");
        }

        function ListadoPaquetesPorFiltro() {

            var TipoPaquete = $("#ddlTipoPaquete").data('kendoDropDownList').value();
            var Busqueda = $("#txtBusquedaPaquetes").val();
            var Estado = $("#ddlEstadoPaquete").data('kendoDropDownList').value();

            $("#gridPaquetes").empty();
            $("#gridPaquetes").kendoGrid({
                dataSource: {
                    type: "json",
                    transport: {
                        read: function (options) {
                            $.ajax({
                                data: '{"Estado":"' + Estado + '","TipoPaquete":"' + TipoPaquete + '","Busqueda":"' + Busqueda + '","PageNumber":"' + 1 + '"}',
                                type: "POST",
                                url: "/operacionesfit/uspListarPaquetesMenbresiasCursos_Paginacion",
                                contentType: "application/json; charset=utf-8",
                                dataType: "json",
                                success: function (msg) {
                                    options.success(msg);
                                }, complete: function () {
                                    uspListarPaquetesMenbresiasCursos_NumeroRegistros();
                                }
                            });
                        }
                    }
                },
                height: 1300,
                selectable: "row",
                columns: [{
                    field: "DesTiempoMembresia",
                    title: "<b style='font-size:12px;'>Tiempo de plan</b>",
                    width: 10,
                    attributes: {
                        style: "font-size:13px;"
                    },
                }, {
                    field: "Descripcion",
                    title: "<b style='font-size:12px;'>Nombre del plan</b>",
                    width: 40,
                    attributes: {
                        style: "font-size:14px;"
                    }
                }, {
                    field: "DescFechaCreacion",
                    title: "<b style='font-size:12px;'>Fecha creación</b>",
                    width: 10,
                    attributes: {
                        style: "font-size:13px;"
                    },
                }, {
                    field: "UsuarioCreacion",
                    title: "<b style='font-size:12px;'>Usuario</b>",
                    width: 8,
                    attributes: {
                        style: "font-size:13px;"
                    },
                }, {
                    field: "DesEstado",
                    title: "<b style='font-size:12px;'>Estado</b>",
                    width: 8,
                    attributes: {
                        style: "font-size:13px;"
                    },
                }
                ],
                dataBound: function (e) {
                    var count = 0;
                    $('#gridPaquetes tbody tr').each(function () {
                        count += 1;
                    });
                    if (count > 0) {
                        var grid = $("#gridPaquetes").data("kendoGrid");
                        this.element.find('tbody tr:first').addClass('k-state-selected');
                        this.select(this.tbody.find('>tr:first'))
                    } else {
                        document.getElementById('btnNuevoPaquete').click();
                        $('#hdCodPaquete').val(0);
                        $("#gridDiasCursos").empty();
                    }

                },
                change: function () {
                    var grid = this;
                    grid.select().each(function () {
                        var dataItem = grid.dataItem($(this));
