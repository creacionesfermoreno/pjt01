<section class="appointment top-space" id="register">
    <div class="container">

        <div class="row">
            <input type="hidden" id="hdSesionFormulario_Imagen" value="@Model.SesionFormulario_Imagen" />
            <div class="col-sm-6 col-md-7">
                <div class="section-title left-aligned">
                    <h2 style="color:var(--colorPrincipal);"><input type="text" id="txtSesionFormulario_Titulo" value="@Model.SesionFormulario_Titulo" class="" style="text-align:center;background-color:transparent;height:50px;width:600px;border: 1px solid #caced1;font-size:35px;font-weight:bold;padding-left:10px;"></h2>
                    <h3 class="subline" style="color:#636363;"><input type="text" id="txtSesionFormulario_Descripcion" value="@Model.SesionFormulario_Descripcion" class="" style="text-align:center;background-color:transparent;height:50px;width:600px;border: 1px solid #caced1;font-size:35px;font-weight:bold;padding-left:10px;"></h3>
                    <hr>
                </div>
                <a class="btn btn-success btn-xs" href="@Url.Action("aparienciapaginaweb_bannerreserva", "operacionesfit", new { id = HttpContext.Current.Request.Cookies["_SubDominio_Business"].Value })">Cambiar imagenes de fondo 653x435</a>
                <button class="btn btn-success btn-xs" onclick="uspActualizarEdicionPaginaWeb();">Guardar cambios</button>
                <div class="custom-control custom-checkbox-toggle custom-control-inline mr-1">
                    @if (Model.SesionFormulario_Estado)
                    {
                        <input checked type="checkbox" id="chkSesionFormulario_Estado" class="custom-control-input" style="height:25px;width:25px;">
                    }
                    else
                    {
                        <input type="checkbox" id="chkSesionFormulario_Estado" class="custom-control-input" style="height:25px;width:25px;">
                    }
                    <label class="custom-control-label" for="chkSesionFormulario_Estado" style="color:#000;">Mostrar Sesión Formulario</label>
                </div>

                <img src="@Model.SesionFormulario_Imagen" class="img-responsive" alt="" />

            </div>
            <!-- // end .col -->

            <div class="col-sm-6 col-md-5">

                <div class="appointment-form top-space-lg">

                        <div style="background-color:var(--colorPrincipal);border: 1px var(--colorPrincipal) solid;" class="form-header">
                            <h4><input type="text" id="txtSesionFormulario_Tituloformulario" value="@Model.SesionFormulario_Tituloformulario" class="" style="text-align:center;background-color:transparent;height:50px;width:400px;border: 1px solid #caced1;font-size:25px;padding-left:10px;font-weight:bold;"></h4>
                        </div>

                        <div class="form-group" style="padding-left:15px;padding-right:15px;">
                            <label style="color:#000;">Nombre completo</label>
                            <input id="txtSesionFormulario_NombreCompleto" type="text" class="form-control" style="border: 1px solid #caced1;font-size:18px;font-weight:bold;padding-left:10px;" required title="Ingrese nombres...">
                        </div>
                        <div class="form-group" style="padding-left:15px;padding-right:15px;">
                            <label style="color:#000;">¿Qué objetivo quieres lograr?</label>
                            <select id="ddlSesionFormulario_Objetivo" class="form-control" style="border: 1px solid #caced1;">
                                <option value="0">Seleccionar</option>
                                <option value="1" >Bajar de peso</option>
                                <option value="2" >Aumentar masa muscular</option>
                                <option value="3" >Mejorar la salud</option>
                                <option value="4" >Superar el stress</option>
                                <option value="5" >tonificar mi cuerpo</option>
                                <option value="6" >Preparación fisica</option>
                            </select>                            
                        </div>
                        <div class="form-group" style="padding-left:15px;padding-right:15px;">
                                <label style="color:#000;">¿En cuanto tiempo te gustaria lograrlo?</label>
                                <select id="ddlSesionFormulario_Promocion" class="form-control" style="border: 1px solid #caced1;">
                                    <option value="0">Seleccionar</option>
                                    @{if (Model.List_Planes != null)
                                        {
                                            foreach (var itemPlanes in Model.List_Planes)
                                            {
                                                <option >@itemPlanes.Titulo</option>                                     
                                            }
                                        }
                                    }
                                </select>                                                           
                        </div>
                        <div class="text-center top-space" style="padding-left:15px;padding-right:15px;">
                            <button onclick="EnviarDatos();" style="background-color:var(--colorPrincipal);color:#fff;border: 1px var(--colorPrincipal) solid;" type="submit" class="btn btn-block btn-xl" id="js-contact-btn">Enviar información</button>                            
                            <div id="js-validacion" style="color:red;display:none;"></div>
                        </div>
                        <br />
                        <div class="form-group" style="padding-left:15px;padding-right:15px;">
                            <div class="col-4">
                                Seleccionar país de destino
                                <select id="ddlSesionFormulario_Pais" class="form-control select2">
                                    <option value="0">Selecconar</option>
                                    <option value="51" selected="selected">Perú (+51)</option>
                                    <option value="593">Ecuador (+593)</option>
                                </select>
                            </div>
                            <div class="col-8" style="color:#000;">
                                Ingrese número de whatsapp de destino
                                <input id="txtSesionFormulario_Whatsapp" value="@Model.SesionFormulario_Whatsapp" type="text" class="form-control" style="border: 1px solid #caced1;font-size:18px;font-weight:bold;padding-left:10px;" placeholder="los datos seran enviados a este nro whatsapp...">
                            </div>
                        </div>

                </div>

            </div>
            <!-- // end .col -->

        </div>
        <!-- // end .row -->

    </div>
    <!-- end .container -->
</section>

