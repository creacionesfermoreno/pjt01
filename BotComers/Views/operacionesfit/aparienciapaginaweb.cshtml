        </div>

        <div class="row">
            @{if (Model.List_Planes != null)
                {
                    foreach (var itemPlanes in Model.List_Planes)
                    {
                        <div class="col-md-4">
                            <div class="pricing-item" data-wow-duration="2s">
                                <div style="color:#000;" class="plan-name">@itemPlanes.Titulo</div>
                                <div style="color:#000;" class="price"> <span class="curr">S/</span>@itemPlanes.SubTitulo</div>
