                }
                else
                {
                    <input type="checkbox" id="chkSesionPlan_Estado" class="custom-control-input" style="height:25px;width:25px;">
                }
                <label class="custom-control-label" for="chkSesionPlan_Estado" style="color:#000;">Mostrar Sesión Precios</label>
            </div>

            <h2 style="color:var(--colorPrincipal);"><input type="text" id="txtSesionPlan_Titulo" value="@Model.SesionPlan_Titulo" class="" style="text-align:center;background-color:transparent;height:50px;width:600px;border: 1px solid #caced1;font-size:35px;font-weight:bold;padding-left:10px;"></h2>
