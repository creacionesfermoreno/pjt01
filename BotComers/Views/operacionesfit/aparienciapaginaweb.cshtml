                                {
                                    <a style="border:1px solid var(--colorPrincipal);background-color:var(--colorPrincipal);color:#fff" class="btn btn-outline btn-lg" href="#register">LO QUIERO</a>
                                }
                                else
                                {
                                    <a style="border:1px solid var(--colorPrincipal);background-color:var(--colorPrincipal);color:#fff" class="btn btn-outline btn-lg" href="@itemPlanes.LinkPago">COMPRAR AHORA</a>
                                }
                            </div>
                        </div>
                    }
                }
                else
