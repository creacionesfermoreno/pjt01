        function EnviarDatos() {
            var nombres = $('#txtSesionFormulario_NombreCompleto').val();
            var objetivo = $('select[id="ddlSesionFormulario_Objetivo"] option:selected').text();
            var tiempo = $('select[id="ddlSesionFormulario_Promocion"] option:selected').text();
            var pais = $('select[id="ddlSesionFormulario_Pais"] option:selected').val();
            var numero = $('#txtSesionFormulario_Whatsapp').val();

            if (nombres == '') {
                $('#js-validacion').html('Falta ingresar nombres y apellidos');
                document.getElementById("js-validacion").style.display = 'block';
                return;
            } else if (objetivo == 'Seleccionar') {
                $('#js-validacion').html('Falta seleccionar su objetivo');
                document.getElementById("js-validacion").style.display = 'block';
                return;
            } else if (tiempo == 'Seleccionar') {
                $('#js-validacion').html('Falta seleccionar el tiempo');
                document.getElementById("js-validacion").style.display = 'block';
                return;
            } else if (pais == 'Seleccionar') {
                $('#js-validacion').html('Falta seleccionar el pais de destino');
                document.getElementById("js-validacion").style.display = 'block';
                return;
            } else if (numero == '') {
                $('#js-validacion').html('Falta configurar el número de destino');
                document.getElementById("js-validacion").style.display = 'block';
                return;
            }

            document.getElementById("js-validacion").style.display = 'none';

            nombres = nombres.replace(' ', '%20').replace(' ', '%20').replace(' ', '%20');
            objetivo = objetivo.replace(' ', '%20').replace(' ', '%20').replace(' ', '%20');
            tiempo = tiempo.replace(' ', '%20').replace(' ', '%20').replace(' ', '%20');
            //alert("nombres: " + nombres);
            //alert("objetivo: " + objetivo);
            //alert("tiempo: " + tiempo);
            //alert("pais: " + pais);
            //alert("numero: " + numero);

            var Link = "https://api.whatsapp.com/send?phone=" + pais + numero + "&text=👍%20Hola,%20mi%20nombre%20es%20" + nombres + ",%20😉%20me%20interesa%20" + objetivo + "%20en%20" + tiempo +"%20💪";
            window.open(Link, '_blank');
            
