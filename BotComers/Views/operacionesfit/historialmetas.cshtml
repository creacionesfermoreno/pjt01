        function CalcularMetaSemanal() {
            var DiaFinal = kendo.toString($("#txtFechaFinMeta").data('kendoDatePicker').value(), 'dd');
            var FechaInicio = kendo.toString($("#txtFechaInicioMeta").data('kendoDatePicker').value(), 'dd');
            var Operacion = ((DiaFinal - FechaInicio) + 1) / 7;
            var operacioEntero = (Operacion.toString()).split('.')[0];
            $('#gridVendedor .k-grid-content table tbody tr').each(function () {
                var Meta = $(this).find("td").eq(3).find("input").val() == '' ? 0 : parseFloat($(this).find("td").eq(3).find("input").val());
                var Resultado = Meta / (parseInt(operacioEntero));
                $(this).find("td").eq(4).find("input").val(Resultado);
            });

        }

