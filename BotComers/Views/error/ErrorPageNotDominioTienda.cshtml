
@{
    ViewBag.Title = "pagina no existe";
}

<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="Ansonika">
    <title>@ViewBag.Title</title>
   

    <!-- GOOGLE WEB FONT -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900&display=swap" rel="stylesheet">

    <!-- BASE CSS -->
    <link href="@Url.Content("~/Content/tienda2/css/bootstrap.custom.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/tienda2/css/style.css")" rel="stylesheet" />
    <!-- SPECIFIC CSS -->
    <link href="@Url.Content("~/Content/tienda2/css/error_track.css")" rel="stylesheet" />

    <!-- YOUR CUSTOM CSS -->
    <link href="@Url.Content("~/Content/tienda2/css/custom.css")" rel="stylesheet" />

</head>
<body>
    <div id="page">

        <main class="bg_gray">
            <div id="error_page">
                <div class="container">
                    <div class="row justify-content-center text-center">
                        <div class="col-xl-7 col-lg-9">
                            <img src="~/Content/tienda2/img/404.svg" alt="" class="img-fluid" width="400" height="212">
                            <p>¡La página que estás buscando no está constrida!</p>
                            <p>Consulta con el administrador de la empresa</p>
                        </div>
                    </div>
                    <!-- /row -->
                </div>
                <!-- /container -->
            </div>
            <!-- /error_page -->
        </main>
       
    </div>

    <div id="toTop"></div><!-- Back to top button -->
    <!-- COMMON SCRIPTS -->
    @System.Web.Optimization.Scripts.Render("~/Content/tienda2/js/common_scripts.min.js")
    @System.Web.Optimization.Scripts.Render("~/Content/tienda2/js/main.js")


    
</body>
</html>



